#category {
  height: 100vh;
  display: flex;
  width: 100vw;
  overflow: hidden;

}

.left {
  flex: 1;
  height: 100vh;
}

.right {
  flex: 2;
  height: 100vh;
  overflow-y: scroll;
  overflow-x: hidden;
}
/* 返回顶部的按钮 */
.com-back-top{
  position: fixed;
  right: 20rpx;
  bottom: 20rpx;
  z-index: 999;
}