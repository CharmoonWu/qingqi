}
.bottom-bar {
  height: 116rpx;
  position: fixed;
  background-color: #fff;
  left: 0;
  right: 0;
  bottom: 0;
  display: flex;
  text-align: center;
  border-top: 1px solid rgb(200,200,200);
}


.bar-left .text {
  font-size: 13px;
}
.bar-item{
  display: flex;
}
.bar-left{
  flex: 1;
  display: flex;
}
.upDown{
  flex: 1;
  display: flex;
  flex-direction: column;
}
.bar-right{
  width: 350rpx;
}
.cart{
  flex: 1;
   line-height: 116rpx;
   background-color: #ffe817;
}
.buy{
  flex: 1;
  line-height: 116rpx;
  background-color: #ff6699;


}
