<!--pages/detail/detail-swiper/detail-swiper.wxml-->

<swiper class="swiper" indicator-dots indicator-active-color='#ff5777' autoplay interval="3000" circular>
  <block wx:for="{{list}}" wx:key="index">
    <swiper-item class="swiper-item">
      <image src="{{item}}" mode="widthFix"></image>
    </swiper-item>
  </block>
</swiper>